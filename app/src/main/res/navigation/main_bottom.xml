    app:startDestination="@id/news">

    <activity
        android:id="@+id/news"
        android:name="com.progetto_ingegneria.pocketvenice.BottomNavbarActivities.News.NewsActivity"
        android:label="News"
        tools:layout="@layout/activity_news" />
    <activity
        android:id="@+id/events"
        android:name="com.progetto_ingegneria.pocketvenice.BottomNavbarActivities.EventsActivity"
        android:label="Events"
        tools:layout="@layout/activity_events" />
    <activity
        android:id="@+id/places"
        android:name="com.progetto_ingegneria.pocketvenice.BottomNavbarActivities.Places.PlacesActivity"
        android:label="Places"
        tools:layout="@layout/activity_places" />
    <activity
        android:id="@+id/map"
